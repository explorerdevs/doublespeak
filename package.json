{
  "name": "doublespeak",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "fmt.lint": "oxlint -D correctness -D perf -A debugger -A eqeqeq --import-plugin --jsx-a11y-plugin --fix -c ./.eslintrc.json",
    "lint": "npm run fmt.lint && npm run fmt",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.5.2",
    "@builder.io/qwik-city": "^1.5.2",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/eslint": "^8.56.7",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-oxlint": "^0.2.8",
    "eslint-plugin-qwik": "^1.5.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "oxlint": "^0.2.16",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.4",
    "undici": "*",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "oxlint",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@qwikest/icons": "^0.0.13",
    "@t3-oss/env-core": "^0.9.2",
    "tailwind-variants": "^0.2.1"
  }
}
